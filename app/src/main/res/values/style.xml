<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="tilStyle">
        <item name="layout_constraintWidth_percent">0.88</item>
        <item name="boxCornerRadiusTopEnd">6dp</item>
        <item name="boxCornerRadiusBottomEnd">6dp</item>
        <item name="boxCornerRadiusTopStart">6dp</item>
        <item name="boxCornerRadiusBottomStart">6dp</item>
        <item name="boxStrokeColor">@color/mintGreen</item>
        <item name="errorIconTint">@color/red</item>
        <item name="boxStrokeErrorColor">@color/red</item>
        <item name="errorTextColor">@color/red</item>
        <item name="boxBackgroundMode">outline</item>
        <item name="boxBackgroundColor">@color/white</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="android:textColorHint">@color/dark_gray</item>
        <item name="startIconTint">@color/mintGreen</item>
        <item name="hintEnabled">true</item>
        <item name="hintAnimationEnabled">true</item>
        <item name="hintTextColor">@color/mintGreen</item>
        <item name="boxStrokeWidthFocused">1.5dp</item>
=        <item name="endIconTint">@color/mintGreen</item>
    </style>

    <style name="ietStyle">
        <item name="maxLines">1</item>
        <item name="android:paddingStart">40dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>
    </style>


    <style name="btnLoginStyle">
        <item name="backgroundTint">@color/mintGreen</item>
        <item name="layout_constraintWidth_percent">0.88</item>
        <item name="android:paddingVertical">15dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="cornerRadius">10dp</item>
    </style>

    <style name="search_box_style">
        <item name="android:layout_margin">16dp</item>
        <item name="android:background">@drawable/ic_search_background</item>
        <item name="android:drawableStart">@drawable/ic_search_icon</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:padding">10dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxLength">45</item>
    </style>

    // Doctor calls fragment styles
    <style name="card_view_style">
        <item name="android:layout_marginHorizontal">16dp</item>
        <item name="android:layout_marginBottom">12dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="cardCornerRadius">10dp</item>
        <item name="cardElevation">5dp</item>
    </style>

    <style name="tv_info_style">
        <item name="android:textColor">@color/dark_gray_text</item>
        <item name="android:textSize">16sp</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="btn_accept_busy_style">
        <item name="layout_constraintWidth_percent">0.36</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="cornerRadius">10dp</item>
        <item name="android:layout_marginBottom">24dp</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="iconGravity">textStart</item>
    </style>

    // case details
    <style name="btn_end_case_style">
        <item name="backgroundTint">@color/red</item>
        <item name="cornerRadius">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:text">@string/end_case</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="tabs_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="backgroundTint">@color/transparent</item>
        <item name="cornerRadius">4dp</item>
        <item name="android:gravity">center</item>
        <item name="strokeColor">@color/gray</item>
        <item name="strokeWidth">0.6dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>



        <style name="tv_details_style">
            <item name="android:layout_width">0dp</item>
            <item name="android:layout_height">wrap_content</item>
            <item name="android:textColor">@color/dark_gray_text</item>
            <item name="android:textSize">14sp</item>
            <item name="android:layout_marginTop">16dp</item>
        </style>

        <style name="tv_Patient_details_style">
            <item name="android:layout_width">0dp</item>
            <item name="android:layout_height">wrap_content</item>
            <item name="android:textColor">@color/black</item>
            <item name="android:textSize">14sp</item>
            <item name="android:gravity">end</item>
        </style>

    <style name="btn_details_add">
        <item name="android:layout_marginTop">20dp</item>
        <item name="backgroundTint">@color/mintGreen</item>
        <item name="cornerRadius">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginBottom">12dp</item>
        <item name="iconGravity">textStart</item>
        <item name="iconSize">35dp</item>
    </style>

    // bottom sheet styles
    <style name="ButtonToggleStyle" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textColor">@drawable/selector_button_background</item>
        <item name="backgroundTint">@android:color/transparent</item>
        <item name="strokeColor">@drawable/selector_button_background</item>
        <item name="strokeWidth">1dp</item>
        <item name="boxStrokeWidthFocused">2dp</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="cornerRadius">10dp</item>
        <item name="iconTint">@drawable/selector_button_background</item>
        <item name="iconGravity">textTop</item>
        <item name="iconPadding">8dp</item>
        <item name="iconSize">30dp</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="btn_request">
        <item name="android:layout_marginVertical">30dp</item>
        <item name="backgroundTint">@color/mintGreen</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="cornerRadius">10dp</item>
    </style>

    <style name="RoundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">4dp</item>
    </style>



</resources>